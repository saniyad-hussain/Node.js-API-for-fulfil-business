{
  "name": "api",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "@google-cloud/pubsub": "^0.20.1",
    "@google-cloud/storage": "^2.5.0",
    "@sailshq/connect-redis": "^3.2.1",
    "@sailshq/eslint": "^4.19.3",
    "@sailshq/lodash": "^3.10.3",
    "@sailshq/socket.io-redis": "^5.2.0",
    "async": "2.0.1",
    "bcrypt-nodejs": "0.0.3",
    "crypto": "^1.0.1",
    "csv-parse": "^4.4.3",
    "elasticsearch": "^15.4.1",
    "grunt": "1.0.1",
    "ip": "^1.1.5",
    "json2csv": "^4.5.2",
    "jsonwebtoken": "^8.5.1",
    "localStorage": "^1.0.4",
    "md5": "^2.2.1",
    "memfs": "2.15.5",
    "moment": "^2.24.0",
    "node-async-loop": "^1.2.2",
    "opsgenie-sdk": "^0.4.3",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "postmates": "^1.0.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sails": "^1.0.2",
    "sails-hook-cron": "^3.0.0",
    "sails-hook-grunt": "^3.0.2",
    "sails-hook-orm": "^2.0.0-16",
    "sails-hook-sockets": "^1.5.5",
    "sails-hook-swagger-generator": "^2.8.2",
    "sails-mock-models": "0.0.6",
    "sails-mongo": "^1.0.1",
    "sails-mysql": "^1.0.0",
    "sails-redis": "^1.0.0",
    "sinon": "^7.3.1",
    "unionfs": "^4.2.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "local": "NODE_ENV=local node app.js",
    "start:staging": "NODE_ENV=staging node app.js",
    "start:development": "node ./node_modules/nodemon/bin/nodemon --ignore .tmp/ --ignore /swagger/ NODE_ENV=development node app.js",
    "test": "node ./node_modules/mocha/bin/mocha test/lifecycle.test.js test/integration/controllers/*.test.js",
    "lint": "eslint . --max-warnings=0 --report-unused-disable-directives && echo '✔  Your .js files look good.'",
    "custom-tests": "echo \"(No other custom tests yet.)\" && echo",
    "probe": "node ./node_modules/mocha/bin/mocha test/lifecycle.test.js test/integration/common/API.test.js",
    "afterjob": "node afterjob.js",
    "docker:development:build": "docker-compose build fulfil-api-development",
    "docker:development:run": "docker-compose up --build fulfil-api-development",
    "staging:test": "node ./node_modules/mocha/bin/mocha test/lifecycle.test.js test/integration/controllers/*.test.js",
    "staging:lint": "eslint . --max-warnings=0 --report-unused-disable-directives --fix && echo '✔  Your .js files look good.'",
    "docker:staging:lint": "docker run gcr.io/fulfil-web/fulfil-api:staging npm run staging:lint",
    "docker:staging:test": "docker run gcr.io/fulfil-web/fulfil-api:staging npm run staging:test",
    "docker:staging:afterjob": "docker run gcr.io/fulfil-web/fulfil-api:staging npm run afterjob",
    "docker:staging:build": "docker-compose build fulfil-api-staging",
    "docker:staging:run": "docker-compose up --build fulfil-api-staging",
    "docker:staging:push": "docker push gcr.io/fulfil-web/fulfil-api:staging",
    "docker:production:afterjob": "docker run gcr.io/fulfil-web/fulfil-api:latest npm run afterjob",
    "docker:production:build": "docker-compose build fulfil-api",
    "docker:production:run": "docker-compose up --build fulfil-api",
    "docker:production:push": "docker push gcr.io/fulfil-web/fulfil-api",
    "write:log": "git --no-pager show --output=assets/version.txt --pretty=fuller",
    "image-cleanup": "bash ./image-cleanup.sh gcr.io/fulfil-web/fulfil-api",
    "delete:tag:staging": "bash ./delete-image-tag.sh staging",
    "delete:tag:latest": "bash ./delete-image-tag.sh latest",
    "staging:replace": "gcloud compute instance-groups managed rolling-action replace fulfil-api-staging-docker-instance-group --zone=us-central1-a --quiet"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/matt/api.git"
  },
  "author": "matt",
  "license": "",
  "engines": {
    "node": ">=6.9"
  },
  "devDependencies": {
    "mocha": "^6.0.2",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.0"
  }
}
